@{
    ViewBag.Title = "User Management";

    Layout = "~/Views/Shared/_Layout.cshtml";

}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">

    <div class="col-md-4">
        <h2>Manage Projects</h2>
        <p>
            View a list of the current projects along with their information.
        </p>
        <p><a class="btn btn-default" href="@Url.Action("Index", "Projects")">View &raquo;</a></p>
    </div>
    <div class="col-md-4">
        <h2>Manage Tickets</h2>
        <p>
            View a list of the current tickets along with their information.
        </p>
        <p><a class="btn btn-default" href="@Url.Action("Index", "Tickets")">View &raquo;</a></p>
    </div>
    <div class="col-md-4">
        <h2>Manage Users</h2>
        <p>
            View a list of the current users along with their information.
        </p>
        <p><a class="btn btn-default" href="@Url.Action("UserList", "Admin")">View &raquo;</a></p>
    </div>
</div>